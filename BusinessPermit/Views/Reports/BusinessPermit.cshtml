@model IEnumerable<BusinessPermit.Models.BusinessPermit>


@{
    ViewBag.Title = "Reports";
    ViewBag.CurrentEntity = "Business Permit";
    ViewBag.WebPageTitle = "Business Permit Reports";
}

@section toolbarleft {
    <li class="breadcrumb-item"><a href="~/Dashboard/Index">Home</a></li>
    <li class="breadcrumb-item active">Business Permit List</li>
}

@section ToolbarRight {
    <li><a href="~/Reports/" title="Payments"><span class="fa fa-money"></span>&nbsp;Payments</a></li>
    <li><a href="~/Reports/BusinessPermit" title="Business Permit"><span class="fa fa-briefcase"></span>&nbsp;Business</a></li>
    <li><a href="~/Reports/BuildingPermit" title="Building Permit"><span class="fa fa-building"></span>&nbsp;Building</a></li>
}

<table class="table">
    <thead>
        <tr>
            <th>
                Is New
            </th>
            <th>
                Account Number
            </th>
            <th>
                Business Name
            </th>
            <th>
                Owner Name
            </th>
            <th>
                Business Nature
            </th>
            <th>
                Assessed Amount
            </th>         
            <th>
                Status
            </th>
           
        </tr>
    </thead>

    <tbody>


        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @Html.DisplayFor(modelItem => item.IsNew)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BusinessAccountNumber)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BusinessName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.OwnerName)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.BusinessNature)
                </td>
                <td>
                    @Html.DisplayFor(modelItem => item.AssessedAmount)
                </td>

                <td>
                    @Html.DisplayFor(modelItem => item.Status)
                </td>
              
            </tr>
        }
    </tbody>
</table>

@section scripts {
    <script src="~/dist/js/tableExport.js"></script>
    <script src="~/dist/js/ga.js"></script>
    <script>
        $(document).ready(function () {
            $('.table').bootstrapTable({ search: true, showExport: true, showToggle: true, showColumns: true });
        });
    </script>
}
