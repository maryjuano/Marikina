@model IEnumerable<BusinessPermit.Models.Fee>

@{
    ViewBag.Title = "Fee";
    ViewBag.CurrentEntity = "Fee";
    ViewBag.WebPageTitle = "Fee List";
}


@section toolbarleft {
    <li class="breadcrumb-item"><a href="~/">Home</a></li>
    <li class="breadcrumb-item active">Fee List</li>
}
@section ToolbarRight {
    <li><a href="~/Fee/Create" title="New User"><span class="fa fa-plus"></span>&nbsp;NEW</a></li>
}

<table class="table table-hover">
    <thead>
    <th>
        @Html.DisplayNameFor(model => model.Description)
    </th>
    <th>
        Application Type
    </th>
    <th>
        @Html.DisplayNameFor(model => model.Price)
    </th>
    <th>
        Created On
    </th>
    <th>
        Created By
    </th>
    <th></th>
    </thead>

    @foreach (var item in Model)
    {
        <tr>
            <td>
                @Html.DisplayFor(modelItem => item.Description)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.ApplicationType.Description)
            </td>
            <td>
                @Html.DisplayForCurrency(item.Price)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedOn)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.CreatedBy.FullName)
            </td>
            <td>
                @Html.LinkWithIcon("Fee", "Edit", item.FeeId, "fa fa-pencil")
                @Html.LinkWithIcon("Fee", "Details", item.FeeId, "fa fa-info")
            </td>
        </tr>
    }

</table>
